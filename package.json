{
	"name": "ui-sandbox",
	"version": "1.0.0",
	"description": "Simple UI sandbox for trying out stuff.",
	"scripts": {
		"server": "nodemon ./src/server/main.js --env=development",
		"build": "webpack --mode=development",
		"watch": "webpack --mode=development --watch",
		"sasslint": "sass-lint --verbose --no-exit ./src/**/*.scss",
		"eslint": "eslint ./src/**/*.js"
	},
	"nodemonConfig": {
		"ignore": [
			"src/client/*"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/barnabas-lesti/ui-sandbox.git"
	},
	"author": "Barnabas Lesti <barnabas.lesti@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"babel-loader": "^8.0.4",
		"css-loader": "^2.0.2",
		"eslint": "^5.11.0",
		"eslint-config-esnext": "^4.0.0",
		"eslint-config-recommended": "^4.0.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-sort-requires-by-path": "^1.0.2",
		"extract-loader": "^3.1.0",
		"file-loader": "^3.0.1",
		"node-sass": "^4.11.0",
		"nodemon": "^1.18.9",
		"sass-lint": "^1.12.1",
		"sass-loader": "^7.1.0",
		"webpack": "^4.28.2",
		"webpack-cli": "^3.1.2"
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"config": "^3.0.1",
		"express": "^4.16.4",
		"express-handlebars": "^3.0.0",
		"fs-extra": "^7.0.1",
		"jquery": "^3.3.1",
		"marked": "^0.5.2",
		"yargs": "^12.0.5"
	}
}
